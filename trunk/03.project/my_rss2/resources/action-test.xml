<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="cw-test" extends="cw-default">
		<action name="rss_index" class="com.cathywu.test.action.RssIndexAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="index" class="com.cathywu.test.action.IndexAction">
			<result name="success">/home.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="rss_detail" class="com.cathywu.test.action.RssItemListAction">
			<result name="success">/rss_item_list.jsp</result>
			<result name="success2">/rss_item_list2.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>

</struts>
